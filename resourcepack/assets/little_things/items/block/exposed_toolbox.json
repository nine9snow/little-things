{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "index": 0,
    "cases": [
      {
        "when": "closed",
        "model": {
          "type": "minecraft:model",
          "model": "little_things:block/toolbox/exposed/closed"
        }
      },
      {
        "when": "opened",
        "model": {
          "type": "minecraft:model",
          "model": "little_things:block/toolbox/exposed/opened"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "little_things:block/toolbox/exposed/closed"
    }
  }
}